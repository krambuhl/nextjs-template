{
  "presets": [
    "next/babel"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": [
          "./"
        ],
        "alias": {
          "@components": "./components",
          "@utils": "./components/01-utils",
          "@atoms": "./components/02-atoms",
          "@molecules": "./components/03-molecules",
          "@organisms": "./components/04-organisms",
          "@templates": "./components/05-templates",
          "@contexts": "./contexts",
          "@hooks": "./hooks",
          "@pages": "./pages",
          "@static": "./static",
          "@styles": "./styles"
        }
      }
    ]
  ]
}
